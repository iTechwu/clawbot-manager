{
  "name": "@repo/utils",
  "version": "0.1.0",
  "private": true,
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "build": "tsc -p tsconfig.build.json && tsc -p tsconfig.build.esm.json",
    "dev": "tsc -p tsconfig.build.json --watch",
    "clean": "rimraf dist",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/esm/index.js",
      "require": "./dist/index.js",
      "default": "./dist/index.js"
    },
    "./cn": {
      "types": "./dist/cn.d.ts",
      "default": "./dist/cn.js"
    },
    "./file": {
      "types": "./dist/file.d.ts",
      "default": "./dist/file.js"
    },
    "./encrypt": {
      "types": "./dist/encrypt.d.ts",
      "default": "./dist/encrypt.js"
    },
    "./fetch": {
      "types": "./dist/fetch.d.ts",
      "default": "./dist/fetch.js"
    },
    "./headers": {
      "types": "./dist/headers.d.ts",
      "default": "./dist/headers.js"
    },
    "./array": {
      "types": "./dist/array.util.d.ts",
      "default": "./dist/array.util.js"
    },
    "./bigint": {
      "types": "./dist/bigint.util.d.ts",
      "default": "./dist/bigint.util.js"
    },
    "./ip": {
      "types": "./dist/ip.util.d.ts",
      "default": "./dist/ip.util.js"
    },
    "./json": {
      "types": "./dist/json.util.d.ts",
      "default": "./dist/json.util.js"
    },
    "./object": {
      "types": "./dist/object.util.d.ts",
      "default": "./dist/object.util.js"
    },
    "./string": {
      "types": "./dist/string.util.d.ts",
      "default": "./dist/string.util.js"
    },
    "./timer": {
      "types": "./dist/timer.util.d.ts",
      "default": "./dist/timer.util.js"
    },
    "./urlencode": {
      "types": "./dist/urlencode.util.d.ts",
      "default": "./dist/urlencode.util.js"
    },
    "./validate": {
      "types": "./dist/validate.util.d.ts",
      "default": "./dist/validate.util.js"
    },
    "./serialize": {
      "types": "./dist/serialize.util.d.ts",
      "default": "./dist/serialize.util.js"
    },
    "./bcrypt": {
      "types": "./dist/bcrypt.util.d.ts",
      "default": "./dist/bcrypt.util.js"
    }
  },
  "dependencies": {
    "@repo/config": "workspace:*",
    "@repo/constants": "workspace:*",
    "@repo/types": "workspace:*",
    "clsx": "^2.0.0",
    "crypto-js": "^4.2.0",
    "lodash": "^4.17.23",
    "moment-timezone": "^0.5.48",
    "pinyin-pro": "^3.28.0",
    "tailwind-merge": "^2.0.0",
    "uuid": "^11.1.0"
  },
  "optionalDependencies": {
    "bcrypt": "^6.0.0"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/crypto-js": "^4.2.2",
    "@types/jest": "^29.5.12",
    "@types/lodash": "^4.17.21",
    "@types/minimatch": "^6.0.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.3",
    "typescript": "^5"
  }
}
